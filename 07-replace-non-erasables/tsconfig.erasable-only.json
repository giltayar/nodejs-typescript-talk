{
  "compilerOptions": {
    "module": "nodenext",
    "target": "es2024",
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "rewriteRelativeImportExtensions": true,
    "erasableSyntaxOnly": true,
    "verbatimModuleSyntax": true,
    "experimentalDecorators": false
  },
  "include": ["src/**/*.ts"]
}
